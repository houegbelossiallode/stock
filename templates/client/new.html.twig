{% extends 'template.html.twig' %}

{% block title %}Enregister un client{% endblock %}

{% block pagetitle %}Nouveau Client{% endblock %}

{% block body %}


<div class="card">
<div class="p-4 p-sm-5">

{{ form_start(form) }}
<div class="row">

<div class="col-6">{{ form_row(form.nom) }}</div>
<div class="col-6"> {{ form_row(form.prenom) }} </div>

</div>

<div class="row">

<div class="col-12">{{ form_row(form.telephone) }}</div>


</div>



{{ form_rest(form) }}

<a href="{{path('app_client')}}">Listes des clients</a>
{% endblock %}
